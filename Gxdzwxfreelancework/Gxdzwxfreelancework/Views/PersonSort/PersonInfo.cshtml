@{

    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
    <meta name="format-detection" content="telephone=no">
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />
    <meta http-equiv="pragma" content="no-cache" />
    <meta http-equiv="cache-control" content="no-cache" />
    <meta http-equiv="expires" content="0" />
    <meta charset="utf-8">
    <title>电子联盟自由职业</title>

    @*<link href="@Url.Content("~/gxfreelance/statics/css/ectouch.css")" rel="stylesheet" />*@
    @*<link href="~/gxfreelance/statics/css/photoswipe.css" rel="stylesheet" />*@

    @*<script src="@Url.Content("~/Scripts/Jquery/jquery-1.10.2.js")"></script>

    <script src="@Url.Content("~/gxfreelance/statics/js/swiper.min.js")"></script>*@

    <link href="~/gxfreelance/statics/css/bootstrap.min.css" rel="stylesheet" />

    <script src="@Url.Content("~/gxfreelance/statics/js/ectouch.js")"></script>

    <script src="@Url.Content("~/gxfreelance/statics/js/jquery.json.js")"></script>

    <link href="@Url.Content("~/gxfreelance/statics/css/style.css")" rel="stylesheet" />

    <link href="@Url.Content("~/gxfreelance/statics/css/user.css")" rel="stylesheet" />

    @*<script src="@Url.Content("~/gxfreelance/statics/js/jquery-ui-1.10.1.custom.min.js")"></script>*@

    <script src="@Url.Content("~/gxfreelance/statics/js/jquery.scrollUp.min.js")"></script>

    @*<script type="text/javascript" src="@Url.Content("https://cdn.bootcss.com/jqueryui-touch-punch/0.2.3/jquery.ui.touch-punch.min.js")"></script>*@

    @*<script src="@Url.Content("~/gxfreelance/statics/js/jquery.lazyload.min.js")"></script>*@
    @*<link href="~/gxfreelance/statics/css/my_style.css" rel="stylesheet" />*@
    <script src="@Url.Content("~/gxfreelance/statics/js/common.js")"></script>
    <script src="@Url.Content("~/gxfreelance/statics/js/jquery.more.js")"></script>
    <script src="@Url.Content("~/Scripts/packpageindex/SendPackage.js")"></script>
    <script src="@Url.Content("~/gxfreelance/statics/js/jquery-2.1.4.min.js")"></script>
    <link href="@Url.Content("~/weui-master/dist/style/weui.css")" rel="stylesheet" />
    <link href="@Url.Content("~/weui-master/dist/style/weui.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/weui-master/dist/example/example.css")" rel="stylesheet" />
    <script src="@Url.Content("~/weui-master/dist/example/example.js")"></script>
</head>
<body style="max-width:640px;">
    <div class="page">
        <div class="ect-bg">
            <header class="ect-header ect-margin-tb ect-margin-lr text-center ect-bg icon-write">
                <a href="@Url.Action("GxFreelanceWxClassification","Home")" class="pull-left ect-icon ect-icon1 ect-icon-history"></a>
                <span>信息详情</span>
            </header>
        </div>
        <div id="show_chathead" class="photo-container" style="margin-top:10px;text-align:center;background-color:white">
            @*<img src="@Url.Content("~/gxfreelance/statics/img/1111111111.png")" style="height:100px;width:auto" />*@
        </div>
        <div class="bd">
            <div class="weui_cells">
                <div class="weui_cell">
                    <div class="weui_cell_bd weui_cell_primary">
                        <p>昵称：</p>
                    </div>
                    <div class="weui_cell_ft">@Session["user_name"]</div>
                </div>
                <div class="weui_cell">
                    <div class="weui_cell_bd weui_cell_primary">
                        <p>性别：</p>
                    </div>
                    <div class="weui_cell_ft">@Session["sex"]</div>
                </div>
                <div class="weui_cell">
                    <div class="weui_cell_bd weui_cell_primary">
                        <p>编号：</p>
                    </div>
                    <div class="weui_cell_ft">@Session["user_id"]</div>
                </div>
                <div class="weui_cell">
                    <div class="weui_cell_bd weui_cell_primary">
                        <p>职业：</p>
                    </div>
                    <div class="weui_cell_ft">@Session["profession"]</div>
                </div>
                <div class="weui_cell">
                    <div class="weui_cell_bd weui_cell_primary">
                        <p>职能：</p>
                    </div>
                    <div class="weui_cell_ft">@Session["function"]</div>
                </div>
                <div class="weui_cell">
                    <div class="weui_cell_bd weui_cell_primary">
                        <p>学历：</p>
                    </div>
                    <div class="weui_cell_ft">@Session["education"]</div>
                </div>
                <div class="weui_cell">
                    <div class="weui_cell_bd weui_cell_primary">
                        <p>专注领域：</p>
                    </div>
                    <div class="weui_cell_ft">@Session["field"]</div>
                </div>
                <div class="weui_cell">
                    <div class="weui_cell_bd weui_cell_primary">
                        <p>个人介绍：</p>
                    </div>                    
                </div>
                <article style="margin-left:10px">
                    <p style="margin-left:10px;text-indent:2.0em">@Session["selfintroduction"]</p>
                </article>
            </div>

            <div class="weui_cells weui-btn-area" style="margin-bottom:10px">
                <a class="weui_btn weui_btn_primary" style="background-color:#1cbb7f " href="@Url.Action("GxFreelanceWxClassification","Home")">返      回</a>
            </div>
        </div>
    </div>
    <script type ="text/javascript">

        $(document).ready(function () {
        var domain_name = "http://egov.jinyuc.com";
        var server_imgurl1 = "/gxdzwx/gxdzimages/gxdzwxlogin/personal/chat_head/";
        var chathead="@Session["chathead"]";
        var  domli='<img src="' + domain_name + server_imgurl1 +chathead+ '" style=\"height:100px;width:auto\" />'
        $("#show_chathead").append(domli);
        });
        </script>
</body>
</html>
